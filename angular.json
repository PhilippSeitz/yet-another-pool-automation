      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "view-state-controls": {
      "projectType": "library",
      "root": "libs/view/state/controls",
      "sourceRoot": "libs/view/state/controls/src",
      "prefix": "pool",
      "architect": {
        "lint": {
